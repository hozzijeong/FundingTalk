<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main.Investment.invest_finish">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="128dp">

        <TextView
            android:id="@+id/finish_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="아파트에 투자완료 되었습니다"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="35dp" />

        <TextView
            android:id="@+id/rate_text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="예상수익률 : 8.9%"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="77dp" />

        <TextView
            android:id="@+id/month_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="투자금액 : 5000만원"
            tools:layout_editor_absoluteX="53dp"
            tools:layout_editor_absoluteY="207dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            //버튼을 누르면 메인액티비티로 이동
            <Button
                android:id="@+id/goto_main"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="메인으로"
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="397dp" />

            <Button
                android:id="@+id/goto_custom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="마이페이지로"
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="397dp" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>