<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/loan_bg_3"
    tools:context=".Login.LoginActivity">

    <EditText
        android:id="@+id/wish_loan_et"
        android:layout_width="302dp"
        android:layout_height="32dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="134dp"
        android:background="@drawable/loan_base_frame"
        android:gravity="center_vertical"
        android:hint="만원"
        android:paddingStart="20dp"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/check_limit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wish_loan_tv"
        app:layout_constraintVertical_bias="0.0" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="35dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/house_cost_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_layout">

        <EditText
            android:id="@+id/address_specific"
            android:layout_width="228dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="29dp"
            android:layout_marginEnd="23dp"
            android:fontFamily="@font/nanumbarungothic"
            android:hint="아파트이름 / 전용면적 입력"
            android:textSize="10dp" />

        <Button
            android:id="@+id/check_address"
            android:layout_width="60dp"
            android:layout_height="23dp"
            android:layout_alignBottom="@id/address_specific"
            android:layout_alignParentEnd="true"
            android:background="@drawable/loan_search_3"
            android:fontFamily="@font/nanumbarungothic"
            android:textSize="10dp" />

    </LinearLayout>

    <TextView
        android:id="@+id/before_loan_tv"
        android:layout_width="93dp"
        android:layout_height="15dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/nanumbarungothicbold"
        android:gravity="center_vertical"
        android:text="선순위 대출금액"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/before_loan_et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cost_result" />

    <TextView
        android:id="@+id/house_cost_tv"
        android:layout_width="52dp"
        android:layout_height="15dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="329dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/nanumbarungothicbold"
        android:gravity="center_vertical"
        android:text="담보 가격"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/cost_result"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

    <LinearLayout
        android:id="@+id/address_layout"
        android:layout_width="320dp"
        android:layout_height="24dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="16dp"
        android:weightSum="3"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_tv">

        <Button
            android:id="@+id/address_do"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/loan_check_3"
            android:fontFamily="@font/nanumbarungothic"
            android:hint="시/도 입력"
            android:textSize="10dp" />

        <Button
            android:id="@+id/address_si"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/loan_check_3"
            android:fontFamily="@font/nanumbarungothic"
            android:hint="시/군/구 입력"
            android:textSize="10dp" />

        <Button
            android:id="@+id/address_gu"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/loan_check_3"
            android:fontFamily="@font/nanumbarungothic"
            android:hint="읍/면/동 입력"
            android:textSize="10dp" />
    </LinearLayout>

    <Button
        android:id="@+id/loan_specific_back"
        android:layout_width="15dp"
        android:layout_height="13dp"
        android:layout_marginTop="70dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/address_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/before_loan_et"
        android:layout_width="302dp"
        android:layout_height="32dp"
        android:layout_marginBottom="61dp"
        android:background="@drawable/loan_base_frame"
        android:fontFamily="@font/nanumbarungothic"
        android:gravity="center_vertical"
        android:hint="만원"
        android:paddingStart="20dp"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/wish_loan_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/before_loan_tv" />

    <TextView
        android:id="@+id/wish_loan_tv"
        android:layout_width="93dp"
        android:layout_height="15dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="@font/nanumbarungothicbold"
        android:gravity="center_vertical"
        android:text="대출액"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/wish_loan_et"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/before_loan_et" />

    <TextView
        android:id="@+id/cost_result"
        android:layout_width="302dp"
        android:layout_height="32dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/loan_base_frame"
        android:fontFamily="@font/nanumbarungothic"
        android:gravity="center_vertical"
        android:paddingStart="20dp"
        android:text="만원"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/before_loan_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/house_cost_tv" />

    <TextView
        android:id="@+id/address_tv"
        android:layout_width="52dp"
        android:layout_height="15dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/nanumbarungothicbold"
        android:gravity="center_vertical"
        android:text="주소 입력"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/address_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loan_specific_back" />

    <Button
        android:id="@+id/check_limit"
        android:layout_width="292dp"
        android:layout_height="38dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/loan_search_3_1"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>